{
  "expo": {
    "name": "motivationalquotes",
    "slug": "motivationalquotes",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#04080F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.globalpromotions.motivationalquotes",
      "buildNumber": "1.1.0",
      "config": {
        "googleMobileAdsAppId": ""
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.globalpromotions.motivationalquotes",
      "versionCode": 8,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "ACCESS_NETWORK_STATE"
      ],
      "config": {
        "googleMobileAdsAppId": ""
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
